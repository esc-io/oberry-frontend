{
  "expo": {
    "name": "oberry-frontend",
    "slug": "oberry-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#22242c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon@2x.png",
        "backgroundColor": "#22242c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
